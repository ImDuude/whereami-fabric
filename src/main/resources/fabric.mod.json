{
	"schemaVersion": 1,
	"id": "where_am_i",
	"version": "${version}",
	"name": "Where am I",
	"description": "WhereAmI is a lightweight Mod that allows players to check which structure they are currently inside or which structures exist nearby.",
	"authors": [
		"ImDuude"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/where_am_i/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.whereami.WhereAmI"
		],
		"client": [
			"com.whereami.WhereAmIClient"
		]
	},
	"mixins": [
		"where_am_i.mixins.json",
		{
			"config": "where_am_i.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.3",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	}
}